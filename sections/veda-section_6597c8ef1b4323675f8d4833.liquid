          <div
            class="veda-swiper-thumbnails w:100%"
            style="margin-top: {{ gap }}px"
            data-options="
              {
                spaceBetween: {{ gap }},
                {% if section.settings["VEDA_slider_7"] %}
                  slidesPerView: {{ section.settings["VEDA_slider_7"] }}
                {% else %}
                  slidesPerView: 6
                {% endif %}
              }
            "
          >
            <div class="swiper">
              <div class="swiper-wrapper">
                {% for media1 in productItem.media %}
                  {% if media1.media_type == 'video' or media1.media_type == 'external_video' %}
                    <div class="swiper-slide cur:pointer z:1||af">
                      {% if media1.preview_image.src != blank %}
