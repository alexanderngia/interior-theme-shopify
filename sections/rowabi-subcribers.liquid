<!-- Initialize Swiper -->
<script>
  var swiper = new Swiper(".mySwiperSub", {
    slidesPerView: "1",
    spaceBetween: 30,
    navigation: {
      nextEl: ".swiper-button-next-sub-{{ section.id }}",
      prevEl: ".swiper-button-prev-sub-{{ section.id }}",
    },
    pagination: {
      el: ".swiper-pagination-sub",
      bulletActiveClass: 'rowabi-active'
    },
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', () => {
      const nextBtn = document.querySelector('.swiper-button-next-sub-{{ section.id }}');
      const defaultNext = document.querySelector('#defaultNext-{{ section.id }}');
      const focusNext = document.querySelector('#focusNext-{{ section.id }}');
  
      if (nextBtn && defaultNext && focusNext) {
        nextBtn.addEventListener('focus', () => {
          defaultNext.classList.add('rowabi-hidden');
          focusNext.classList.remove('rowabi-hidden');
        });
  
        nextBtn.addEventListener('blur', () => {
          defaultNext.classList.remove('rowabi-hidden');
          focusNext.classList.add('rowabi-hidden');
        });
      }
  
      const prevBtn = document.querySelector('.swiper-button-prev-sub-{{ section.id }}');
      const defaultPrev = document.querySelector('#defaultPrev-{{ section.id }}');
      const focusPrev = document.querySelector('#focusPrev-{{ section.id }}');
  
      if (prevBtn && defaultPrev && focusPrev) {
        prevBtn.addEventListener('focus', () => {
          defaultPrev.classList.add('rowabi-hidden');
          focusPrev.classList.remove('rowabi-hidden');
        });
  
        prevBtn.addEventListener('blur', () => {
          defaultPrev.classList.remove('rowabi-hidden');
          focusPrev.classList.add('rowabi-hidden');
        });
      }
  });
</script> 
              
{% schema %}
{
  "name": "Rowabi Subcribers",
  "class": "rowabi-subcribers",
  "tag": "section",
  "max_blocks": 10,
  "settings": [
    {
      "type": "select",
      "id": "heading_tag",
      "label": "Heading tag",
      "options": [
        {
          "value": "h1",
          "label": "H1"
        },
        {
          "value": "h2",
          "label": "H2"
        },
        {
          "value": "h3",
          "label": "H3"
        },
        {
          "value": "h4",
          "label": "H4"
        },
        {
          "value": "h5",
          "label": "H5"
        },
        {
          "value": "h6",
          "label": "H6"
        },
        {
          "value": "p",
          "label": "P"
        }
      ],
      "default": "h1"
    },
    {
      "type": "inline_richtext",
      "id": "title",
      "label": "t:global.text.heading",
      "default": "OVER 500,000 SUBSCRIBERS LOVE ROWABI"
    },
    {
      "type": "inline_richtext",
      "id": "subheading",
      "label": "t:global.text.subheading",
      "default": "Here’s what Rowabi customers had to say: "
    },
  ],
  "blocks": [
    {
      "type": "item",
      "name": "t:sections.timeline.blocks.item.name",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "richtext",
          "id": "content",
          "label": "Content"
        },
        {
          "type": "inline_richtext",
          "id": "author",
          "label": "Author"
        },
        {
          "type": "inline_richtext",
          "id": "job",
          "label": "Job"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Rowabi Subcribers",
    }
  ]
}
{% endschema %}